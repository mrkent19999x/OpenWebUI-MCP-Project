  "failover": {
    "enabled": true,
    "strategy": "priority_based",
    "max_retries": 3,
    "retry_delay_seconds": 2,
    "check_quota_before_use": true,
    "auto_switch_on_error": true,
    "error_codes_to_retry": [
      429,
      503,
      500,
      401
    ],
    "log_failover_events": true
  },
  "prompt_routing": {
    "enabled": true,
    "auto_detect_task": true,
    "rules": {
      "code_execution": {
        "detection_keywords": ["run", "execute", "terminal", "command", "code", "python", "bash", "script"],
        "providers": ["groq", "deepseek", "together"],
        "preferred_models": ["llama-3.1-70b-versatile", "deepseek-coder", "codellama/CodeLlama-34b-Instruct-hf"],
        "timeout": 60,
        "sandbox": true
      },
      "file_operations": {
        "detection_keywords": ["create", "file", "folder", "directory", "write", "read", "delete", "edit"],
        "providers": ["groq", "openrouter", "minimax"],
        "preferred_models": ["mixtral-8x7b-32768", "abab-5.5-chat"],
        "timeout": 30,
        "sandbox": true
      },
      "project_creation": {
        "detection_keywords": ["project", "init", "setup", "create project", "new project"],
        "providers": ["groq", "openrouter", "together"],
        "preferred_models": ["llama-3.1-70b-versatile", "mixtral-8x7b-32768"],
        "timeout": 60,
        "sandbox": true
      },
      "general_chat": {
        "detection_keywords": ["chat", "talk", "conversation", "ask", "question"],
        "providers": ["openrouter", "groq", "together", "minimax"],
        "preferred_models": ["auto", "llama-3.1-70b-versatile"],
        "timeout": 30,
        "sandbox": false
      },
      "vietnamese": {
        "detection_keywords": ["tiếng việt", "vietnamese", "việt nam"],
        "providers": ["minimax", "groq", "openrouter"],
        "preferred_models": ["abab-5.5-chat", "glm-4"],
        "timeout": 30,
        "sandbox": false
      },
      "search": {
        "detection_keywords": ["search", "find", "tìm", "lookup", "google"],
        "providers": ["perplexity", "openrouter"],
        "preferred_models": ["llama-3.1-sonar-large-128k-online"],
        "timeout": 60,
        "sandbox": false
      },
      "reasoning": {
        "detection_keywords": ["analyze", "think", "reason", "solve", "calculate"],
        "providers": ["groq", "openrouter", "together"],
        "preferred_models": ["llama-3.1-70b-versatile", "mixtral-8x7b-32768"],
        "timeout": 60,
        "sandbox": false
      }
    }
  },
  "code_execution": {
    "enabled": true,
    "sandbox": true,
    "timeout": 300,
    "memory_limit": "512MB",
    "cpu_limit": 1,
    "allowed_commands": [
      "python",
      "node",
      "bash",
      "sh",
      "git",
      "npm",
      "pip"
    ],
    "blocked_commands": [
      "rm -rf",
      "format",
      "del",
      "delete"
    ],
    "auto_save": true,
    "show_output": true
  },
  "file_operations": {
    "enabled": true,
    "auto_create_directories": true,
    "max_file_size": "10MB",
    "allowed_extensions": [
      ".py",
      ".js",
      ".ts",
      ".json",
      ".md",
      ".txt",
      ".yml",
      ".yaml",
      ".html",
      ".css"
    ],
    "show_preview": true,
    "auto_format": true
  },
  "gui": {
    "terminal": {
      "enabled": true,
      "theme": "dark",
      "show_timestamps": true,
      "max_output_lines": 1000,
      "auto_scroll": true
    },
    "file_tree": {
      "enabled": true,
      "show_hidden": false,
      "icons": true,
      "auto_refresh": true
    },
    "code_editor": {
      "enabled": true,
      "syntax_highlighting": true,
      "line_numbers": true,
      "auto_complete": true,
      "theme": "monokai"
    },
    "notifications": {
      "enabled": true,
      "show_success": true,
      "show_errors": true,
      "show_warnings": true,
      "auto_hide": 5
    }
  }
}
