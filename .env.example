# OpenWebUI v0.6.36 Complete Configuration Template
# Copy this file to .env and fill in your actual values

# ============================================
# BASIC CONFIGURATION
# ============================================
WEBUI_URL=http://localhost:7860
WEBUI_NAME=Open WebUI
DEFAULT_LOCALE=vi
WEBUI_AUTH=True
ENABLE_SIGNUP=True
DEFAULT_USER_ROLE=pending

# ============================================
# SECURITY & AUTHENTICATION
# ============================================
WEBUI_SECRET_KEY=manus-ai-sandbox-secret-2025-change-this
JWT_EXPIRES_IN=4w
ENABLE_STAR_SESSIONS_MIDDLEWARE=True

# OAuth 2.1 Configuration (v0.6.35-v0.6.36)
OAUTH_GROUPS_SEPARATOR=;
OAUTH_ROLES_SEPARATOR=,
ENABLE_OAUTH_WITHOUT_EMAIL=False

# ============================================
# MCP (Model Context Protocol) - GitHub Integration
# ============================================
ENABLE_MCP_INTEGRATION=True
MCP_SERVER_URL=http://mcp-server:8000
MCP_GITHUB_HOST=localhost
MCP_GITHUB_PORT=8000

# GitHub Configuration (for MCP operations)
GITHUB_TOKEN=ghp_your_personal_access_token_here
GITHUB_USERNAME=your_github_username
GITHUB_REPO=OpenWebUI-MCP-Project

# ============================================
# CODE EXECUTION SANDBOX (Manus AI Equivalent)
# ============================================
CODE_EXECUTOR_URL=http://code-executor:5000
EXECUTION_TIMEOUT=300
MAX_MEMORY=512MB
ALLOWED_LANGUAGES=python,nodejs,bash,sh
SECURITY_LEVEL=high

# ============================================
# WEB AUTOMATION (Browser Control)
# ============================================
WEB_AUTOMATION_URL=http://web-automation:3000
BROWSER_TIMEOUT=30000
MAX_CONCURRENT_TABS=10
HEADLESS_MODE=true

# ============================================
# AUTO-DEPLOYMENT CONFIGURATION
# ============================================
DEPLOY_DOMAIN=localhost
SSL_ENABLED=false
DEPLOY_PATH=/var/www/html

# ============================================
# FILE MANAGEMENT
# ============================================
FILE_MANAGER_URL=http://file-manager:9000
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=minioadmin123

# ============================================
# LiteLLM Gateway Configuration (100+ AI Models)
# ============================================
LITELLM_PORT=4000
LITELLM_MASTER_KEY=sk-litellm-master-key-change-this
LITELLM_BASE_URL=http://litellm:4000
ENABLE_LITELLM_GATEWAY=True
LITELLM_MAX_BATCH_SIZE=10
LITELLM_MAX_CONCURRENT_REQUESTS=100
LITELLM_REQUESTS_PER_MINUTE=1000
LITELLM_LOG_LEVEL=INFO

# ============================================
# PostgreSQL Database (for OpenWebUI)
# ============================================
POSTGRES_PORT=5432
POSTGRES_DB=openwebui
POSTGRES_USER=openwebui
POSTGRES_PASSWORD=openwebui-change-this

