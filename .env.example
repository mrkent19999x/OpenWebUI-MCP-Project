# OpenWebUI v0.6.36 Complete Configuration Template
# Copy this file to .env and fill in your actual values

# ============================================
# BASIC CONFIGURATION
# ============================================
WEBUI_URL=http://localhost:3000
WEBUI_NAME=Open WebUI
DEFAULT_LOCALE=vi
WEBUI_AUTH=True
ENABLE_SIGNUP=True
DEFAULT_USER_ROLE=pending

# ============================================
# SECURITY & AUTHENTICATION
# ============================================
WEBUI_SECRET_KEY=change-this-to-a-secure-random-key-256-bits
JWT_EXPIRES_IN=4w
ENABLE_STAR_SESSIONS_MIDDLEWARE=True

# OAuth 2.1 Configuration (v0.6.35-v0.6.36)
OAUTH_GROUPS_SEPARATOR=;
OAUTH_ROLES_SEPARATOR=,
ENABLE_OAUTH_WITHOUT_EMAIL=False

# ============================================
# LLM API KEYS (Free Providers - Auto Failover)
# ============================================
# Priority 1: Groq (FREE - Very Fast)
GROQ_API_KEY=

# Priority 2: OpenRouter (FREE Tier - Aggregation)
OPENROUTER_API_KEY=

# Priority 3: Together AI (FREE Credits - $25)
TOGETHER_API_KEY=

# Priority 4: Hugging Face (FREE - Unlimited)
HUGGINGFACE_API_KEY=

# Priority 5: MiniMax (FREE Credits)
MINIMAX_API_KEY=

# Priority 6: DeepSeek (FREE Tier)
DEEPSEEK_API_KEY=

# Priority 7: Replicate (FREE Credits - $5)
REPLICATE_API_KEY=

# Priority 8: Perplexity (FREE Tier)
PERPLEXITY_API_KEY=

# Other Providers
OPENAI_API_KEY=
GEMINI_API_KEY=

# MiniMax Configuration (OpenAI-Compatible)
MINIMAX_API_KEY=
MINIMAX_API_BASE_URL=https://api.minimax.chat/v1
ENABLE_MINIMAX=True
MINIMAX_SANDBOX_ENABLED=False
MINIMAX_SANDBOX_API_URL=https://api.minimax.chat/v1/sandbox

# ============================================
# IMAGE GENERATION (v0.6.35)
# ============================================
ENABLE_IMAGE_GENERATION=True
GEMINI_API_BASE_URL=https://generativelanguage.googleapis.com/v1beta
ENABLE_GEMINI_2_5_FLASH_IMAGE=True
ENABLE_QWEN_IMAGE_EDIT=True
AUTOMATIC1111_API_URL=
ENABLE_AUTOMATIC1111_JSON_PARAMS=True

# ============================================
# AUDIO/TTS SYSTEM (v0.6.35)
# ============================================
# ElevenLabs TTS
ELEVENLABS_API_BASE_URL=https://api.elevenlabs.io/v1
ELEVENLABS_API_KEY=

# Mistral Voxtral TTS
MISTRAL_OCR_API_BASE_URL=https://api.mistral.ai/v1
MISTRAL_API_KEY=
ENABLE_MISTRAL_VOXTRAL_TTS=True
VOXTRAL_MODEL=voxtral-small
ENABLE_GLOBAL_AUDIO_QUEUE=True
ENABLE_TTS_STOP_CONTROL=True

# Other Audio Services
WHISPER_CLOUD_KEY=
TTS_API_KEY=

# ============================================
# DOCUMENT PROCESSING
# ============================================
# MinerU Integration (v0.6.34)
ENABLE_MINERU=False
MINERU_API_URL=http://mineru:8000
MINERU_API_KEY=

# External Document Loaders (v0.6.35)
ENABLE_FORWARD_USER_INFO_HEADERS=True

# ============================================
# MCP (Model Context Protocol)
# ============================================
ENABLE_DIRECT_CONNECTIONS=True
TOOL_SERVER_CONNECTIONS=[{"type":"mcp","url":"http://orchestrator:8000","path":"/mcp/tools"}]

# ============================================
# DATABASE & CACHING
# ============================================
REDIS_URL=redis://redis:6379
QDRANT_URL=http://qdrant:6333
VECTOR_DB=qdrant

# ============================================
# PERFORMANCE SETTINGS (MiniMax-like Optimization)
# ============================================
ENABLE_REALTIME_CHAT_SAVE=False
CHAT_RESPONSE_STREAM_DELTA_CHUNK_SIZE=10
THREAD_POOL_SIZE=100
MODELS_CACHE_TTL=3600
AIOHTTP_CLIENT_TIMEOUT=600

# Advanced Caching
ENABLE_RESPONSE_CACHE=True
RESPONSE_CACHE_TTL=3600
ENABLE_CONTEXT_CACHE=True
CONTEXT_CACHE_TTL=7200
ENABLE_FILE_OPERATIONS_CACHE=True

# Async Processing (Multi-Agent)
ENABLE_ASYNC_PROCESSING=True
ASYNC_WORKER_THREADS=10
ENABLE_BACKGROUND_TASKS=True
MAX_CONCURRENT_REQUESTS=100

# Streaming
ENABLE_STREAMING=True
STREAMING_CHUNK_SIZE=10
ENABLE_PROGRESSIVE_RENDERING=True

# Connection Pooling
ENABLE_CONNECTION_POOLING=True
CONNECTION_POOL_SIZE=50
CONNECTION_POOL_MAX_OVERFLOW=20

# Memory Optimization
ENABLE_MEMORY_OPTIMIZATION=True
MEMORY_CACHE_SIZE=512MB
ENABLE_MEMORY_POOLING=True

# ============================================
# FEATURES
# ============================================
ENABLE_TITLE_GENERATION=True
ENABLE_FOLLOW_UP_GENERATION=True
ENABLE_MESSAGE_RATING=True
ENABLE_COMMUNITY_SHARING=True
ENABLE_ADMIN_EXPORT=True
ENABLE_ADMIN_CHAT_ACCESS=True
SHOW_ADMIN_DETAILS=True

# Progressive Web App (v0.6.33)
ENABLE_PROGRESSIVE_WEB_APP=True
ENABLE_PWA_ANDROID_SHARE_TARGET=True

# ============================================
# INTEGRATION SERVICES
# ============================================
GOOGLE_VISION_API_KEY=
GMAIL_USER=
GMAIL_APP_PASSWORD=
GITHUB_TOKEN=
ZALO_OA_ACCESS_TOKEN=
ZALO_OA_SECRET_KEY=
ZALO_OA_WEBHOOK_SECRET=
